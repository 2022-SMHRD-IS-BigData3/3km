<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="km/model/MemberDAO.java">

	<insert id="join" parameterType="km.model.MemberVO">
		INSERT INTO USER_INFO
		VALUES(USER_SEQ.nextval,#{email},#{pw},#{birthday},#{nickname},#{dog_name},#{dog_age},#{dog_breed})
	</insert>
	
	<select id="login" parameterType="km.model.MemberVO" resultType="km.model.MemberVO">
  		select *
  		from USER_INFO
  		where email = #{email} and pw = #{pw}
  	</select>















</mapper>